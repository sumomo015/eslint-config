{
  "name": "@sumomo015/eslint-config",
  "version": "1.2.5",
  "type": "module",
  "packageManager": "pnpm@10.27.0+sha512.72d699da16b1179c14ba9e64dc71c9a40988cbdc65c264cb0e489db7de917f20dcf4d64d8723625f2969ba52d4b7e2a1170682d9ac2a5dcaeaab732b7e16f04a",
  "author": "sumomo015",
  "description": "Sumomo015's ESLint config",
  "license": "GPL-3.0-only",
  "keywords": [
    "eslint-config"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sumomo015/eslint-config.git"
  },
  "homepage": "https://github.com/sumomo015/eslint-config",
  "engines": {
    "node": ">=22.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "build": "tsdown",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test:unit": "vitest",
    "test:unit-ui": "vitest --coverage.enabled=true --ui"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@types/node": "^24.10.4",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.39.2",
    "jiti": "^2.6.1",
    "prettier": "^3.7.4",
    "tsdown": "0.18.4",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@eslint/js": "^9.39.2",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-vue": "^10.6.2",
    "typescript-eslint": "^8.51.0",
    "vue-eslint-parser": "^10.2.0"
  }
}
