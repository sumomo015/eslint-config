{
  "name": "@sumomo015/eslint-config",
  "version": "2.0.0",
  "type": "module",
  "packageManager": "pnpm@10.29.1+sha512.48dae233635a645768a3028d19545cacc1688639eeb1f3734e42d6d6b971afbf22aa1ac9af52a173d9c3a20c15857cfa400f19994d79a2f626fcc73fccda9bbc",
  "author": "sumomo015",
  "description": "Sumomo015's ESLint config",
  "license": "GPL-3.0-only",
  "keywords": [
    "eslint-config"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sumomo015/eslint-config.git"
  },
  "homepage": "https://github.com/sumomo015/eslint-config",
  "engines": {
    "node": ">=22.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "build": "tsdown",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test:unit": "vitest",
    "test:unit-ui": "vitest --coverage.enabled=true --ui"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@types/node": "^24.10.13",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "eslint": "^9.39.2",
    "jiti": "^2.6.1",
    "prettier": "^3.8.1",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@eslint-react/eslint-plugin": "^2.12.4",
    "@eslint/js": "^9.39.2",
    "@next/eslint-plugin-next": "^16.1.6",
