{
  "name": "@sumomo015/eslint-config",
  "version": "1.2.2",
  "type": "module",
  "packageManager": "pnpm@10.19.0+sha512.c9fc7236e92adf5c8af42fd5bf1612df99c2ceb62f27047032f4720b33f8eacdde311865e91c411f2774f618d82f320808ecb51718bfa82c060c4ba7c76a32b8",
  "author": "sumomo015",
  "description": "Sumomo015's ESLint config",
  "license": "GPL-3.0-only",
  "keywords": [
    "eslint-config"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sumomo015/eslint-config.git"
  },
  "homepage": "https://github.com/sumomo015/eslint-config",
  "engines": {
    "node": ">=22.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "ncu": "ncu -ui",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "build": "tsdown",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test:unit": "vitest",
    "test:unit-ui": "vitest --coverage.enabled=true --ui"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@vitest/coverage-v8": "^4.0.4",
    "@vitest/ui": "^4.0.4",
    "eslint": "^9.38.0",
    "jiti": "^2.6.1",
    "npm-check-updates": "^19.1.1",
    "prettier": "^3.6.2",
    "tsdown": "^0.15.11",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vitest": "^4.0.4"
  },
  "peerDependencies": {
    "eslint": "^9.23.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@eslint/js": "^9.38.0",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-vue": "^10.5.1",
    "typescript-eslint": "^8.46.2",
    "vue-eslint-parser": "^10.2.0"
  }
}
