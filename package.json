{
  "name": "@sumomo015/eslint-config",
  "version": "1.2.5",
  "type": "module",
  "packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd",
  "author": "sumomo015",
  "description": "Sumomo015's ESLint config",
  "license": "GPL-3.0-only",
  "keywords": [
    "eslint-config"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sumomo015/eslint-config.git"
  },
  "homepage": "https://github.com/sumomo015/eslint-config",
  "engines": {
    "node": ">=22.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "ncu": "ncu -ui",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "build": "tsdown",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test:unit": "vitest",
    "test:unit-ui": "vitest --coverage.enabled=true --ui"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@vitest/coverage-v8": "^4.0.7",
    "@vitest/ui": "^4.0.7",
    "eslint": "^9.39.1",
    "jiti": "^2.6.1",
    "npm-check-updates": "^19.1.2",
    "prettier": "^3.6.2",
    "tsdown": "^0.16.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.0",
    "vitest": "^4.0.7"
  },
  "peerDependencies": {
    "eslint": "^9.23.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@eslint/js": "^9.39.1",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^8.46.3",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-vue": "^10.5.1",
    "typescript-eslint": "^8.46.3",
    "vue-eslint-parser": "^10.2.0"
  }
}
